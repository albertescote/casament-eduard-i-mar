generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Rsvp {
  id         String   @id @default(cuid())
  createdAt  DateTime @default(now())
  fullName   String
  email      String
  attending  Boolean
  notes      String?  @db.Text

  @@index([email])
}


